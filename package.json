{
  "name": "svg-opti-detector",
  "version": "1.0.0",
  "description": "A powerful Node.js tool to analyze, optimize, and detect duplicate inline SVGs in HTML from URLs or local files. Provides detailed optimization reports with color-coded output and potential file size savings.",
  "main": "svg-opti-detector.js",
  "bin": {
    "svg-opti-detector": "./svg-opti-detector.js"
  },
  "files": [
    "svg-opti-detector.js",
    "readme.md",
    "LICENSE"
  ],
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "node svg-opti-detector.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "demo": "node svg-opti-detector.js test/test-svgs-with-classes.html",
    "demo:duplicates": "node svg-opti-detector.js test/test-svgs-with-classes.html --duplicates",
    "prepublishOnly": "pnpm test",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "keywords": [
    "svg",
    "optimization",
    "html",
    "analysis",
    "duplicate-detection",
    "web-performance",
    "file-size",
    "svgo",
    "cli-tool",
    "optimization-report"
  ],
  "author": {
    "name": "Bob Haring",
    "email": "bob.haring@hotmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobharing/svg-opti-detector.git"
  },
  "bugs": {
    "url": "https://github.com/bobharing/svg-opti-detector/issues"
  },
  "homepage": "https://github.com/bobharing/svg-opti-detector#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "cheerio": "^1.1.2",
    "svgo": "^4.0.0"
  },
  "devDependencies": {
    "jest": "^30.2.0"
  },
  "preferGlobal": true
}
